{
    "algo_name": "act",
    "experiment": {
        "name": "test",
        "description": "v1",
        "validate": true,
        "logging": {
            "terminal_output_to_txt": true,
            "log_wandb": true,
            "wandb_proj_name": "ACTSP"
        },
        "save": {
            "enabled": true,
            "every_n_seconds": null,
            "every_n_epochs": 50,
            "top_n": 5,
            "epochs": [],
            "on_best_validation": true,
            "on_best_rollout_return": false,
            "on_best_rollout_success_rate": true,
            "video_freq": 200
        },
        "epoch_every_n_steps": 100,
        "validation_freq": 200,
        "validation_max_samples": 15000,
        "env": null,
        "additional_envs": null,
        "render": false,
        "render_video": true,
        "keep_all_videos": false,
        "video_skip": 5,
        "rollout": {
            "enabled": false,
            "n": 50,
            "horizon": 400,
            "rate": 40,
            "warmstart": 0,
            "terminate_on_success": true
        }
    },
    "train": {
        "data": "./datasets/bowlplace_human.hdf5",
        "data_type": "hand",
        "data_2": null,
        "output_dir":"../trained_models_highlevel",
        "num_data_workers": 5,
        "hdf5_cache_mode": "low_dim",
        "hdf5_use_swmr": true,
        "hdf5_load_next_obs": false,
        "hdf5_filter_key": "train",
        "hdf5_validation_filter_key": "valid",
        "hdf5_normalize_obs": false,
        "hdf5_normalize_actions": false,
        "seq_length": 100,
        "prestacked_actions": true,
        "ac_key": "actions_xyz_act",
        "pad_seq_length": true,
        "frame_stack": 1,
        "pad_frame_stack": true,
        "dataset_keys": [
            "actions_xyz_act"
        ],
        "goal_mode": null,
        "cuda": true,
        "batch_size": 32,
        "num_epochs": 10000,
        "seed": 1
    },
    "algo": {
        "optim_params": {
            "policy": {
                "optimizer_type": "adamw",
                "learning_rate": {
                    "initial": 5e-5,
                    "decay_factor": 1,
                    "epoch_schedule": [
                        100
                    ],
                    "scheduler_type": "linear"
                },
                "regularization": {
                    "L2": 0.0001
                }
            }
        },
        "loss": {
            "l2_weight": 0.0,
            "l1_weight": 1.0,
            "cos_weight": 0.0
        },
        "act": {
            "hidden_dim": 512,
            "dim_feedforward": 3200,
            "backbone": "resnet18",
            "enc_layers": 4,
            "dec_layers": 7,
            "nheads": 8,
            "latent_dim": 32,
            "kl_weight": 20
        },
        "playdata": {
            "enabled": null,
            "goal_image_range": null,
            "eval_goal_gap": null
        }
    },
    "observation": {
        "modalities": {
            "obs": {
                "low_dim": [
                    "ee_pose"
                ],
                "rgb": ["front_img_1_line"],
                "depth": [],
                "scan": []
            },
            "goal": {
                "low_dim": [],
                "rgb": [],
                "depth": [],
                "scan": []
            }
        },
        "encoder": {
            "low_dim": {
                "core_class": null,
                "core_kwargs": {},
                "obs_randomizer_class": null,
                "obs_randomizer_kwargs": {}
            },
            "rgb": {
                "core_class": "VisualCore",
                "core_kwargs": {
                    "feature_dimension": 64,
                    "backbone_class": "ResNet18Conv",
                    "backbone_kwargs": {
                        "pretrained": false,
                        "input_coord_conv": false
                    },
                    "pool_class": "SpatialSoftmax",
                    "pool_kwargs": {
                        "num_kp": 32,
                        "learnable_temperature": false,
                        "temperature": 1.0,
                        "noise_std": 0.0
                    }
                },
                "obs_randomizer_class": "CropRandomizer",
                "obs_randomizer_kwargs": {
                    "size": [240, 320],
                    "scale": [0.9, 1.0],
                    "ratio": [1.33, 1.33],
                    "num_crops": 1,
                    "pos_enc": false,
                    "brightness_min": 0.9,
                    "brightness_max": 1.1,
                    "contrast_min": 0.9,
                    "contrast_max": 1.1,
                    "saturation_min": 0.9,
                    "saturation_max": 1.1,
                    "hue_min": -0.05,
                    "hue_max": 0.05
                }
            },
            "depth": {
                "core_class": "VisualCore",
                "core_kwargs": {},
                "obs_randomizer_class": null,
                "obs_randomizer_kwargs": {}
            },
            "scan": {
                "core_class": "ScanCore",
                "core_kwargs": {},
                "obs_randomizer_class": null,
                "obs_randomizer_kwargs": {}
            }
        }
    }
}